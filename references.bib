}

@article{taylor_autonomous_2017,
  type = {Article},
  title = {Autonomous Model Protocell Division Driven by Molecular Replication},
  author = {Taylor, J. W. and Eghtesadi, S. A. and Points, L. J. and Liu, T. and Cronin, L.},
  year = {2017},
  month = aug,
  journal = {NATURE COMMUNICATIONS},
  volume = {8},
  publisher = {{NATURE PUBLISHING GROUP}},
  address = {{MACMILLAN BUILDING, 4 CRINAN ST, LONDON N1 9XW, ENGLAND}},
  issn = {2041-1723},
  doi = {10.1038/s41467-017-00177-4},
  abstract = {The coupling of compartmentalisation with molecular replication is thought to be crucial for the emergence of the first evolvable chemical systems. Minimal artificial replicators have been designed based on molecular recognition, inspired by the template copying of DNA, but none yet have been coupled to compartmentalisation. Here, we present an oil-in-water droplet system comprising an amphiphilic imine dissolved in chloroform that catalyses its own formation by bringing together a hydrophilic and a hydrophobic precursor, which leads to repeated droplet division. We demonstrate that the presence of the amphiphilic replicator, by lowering the interfacial tension between droplets of the reaction mixture and the aqueous phase, causes them to divide. Periodic sampling by a droplet-robot demonstrates that the extent of fission is increased as the reaction progresses, producing more compartments with increased self-replication. This bridges a divide, showing how replication at the molecular level can be used to drive macroscale droplet fission.},
  affiliation = {Cronin, L (Corresponding Author), Univ Glasgow, WestCHEM Sch Chem, Univ Ave, Glasgow G12 8QQ, Lanark, Scotland. Taylor, J. W.; Points, L. J.; Cronin, L., Univ Glasgow, WestCHEM Sch Chem, Univ Ave, Glasgow G12 8QQ, Lanark, Scotland. Eghtesadi, S. A.; Liu, T., Univ Akron, Dept Polymer Sci, Akron, OH 44325 USA.},
  article-number = {237},
  author-email = {Lee.Cronin@glasgow.ac.uk},
  da = {2022-08-02},
  doc-delivery-number = {FD2YQ},
  funding-acknowledgement = {EPSRC [EP/H024107/1, EP/I033459/1, EP/J00135X/1, EP/J015156/1, EP/K021966/1, EP/K023004/1, EP/K038885/1, EP/L015668/1, EP/L023652/1]; BBSRC [BB/M011267/1]; EC [610730 EVOPROG, 611640 EVOBLISS]; Royal Society/Wolfson Foundation; ERC [670467 SMART-POM]; National Science Foundation [CHE1607138]; EPSRC [EP/F016360/1] Funding Source: UKRI; Engineering and Physical Sciences Research Council [EP/K023004/1, EP/K021966/1, EP/K038885/1, EP/L015668/1, EP/F016360/1, EP/L023652/1] Funding Source: researchfish},
