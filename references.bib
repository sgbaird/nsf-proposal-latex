  author = {Vargas, Santiago and Zamirpour, Siavash and Menon, Shreya and Rothman, Arielle and H{\"a}se, Florian and {Tamayo-Mendoza}, Teresa and Romero, Jonathan and Sim, Sukin and Menke, Tim and {Aspuru-Guzik}, Al{\'a}n},
