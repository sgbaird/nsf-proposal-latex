@article{griffiths_achieving_2022,
  title = {Achieving Robustness to Aleatoric Uncertainty with Heteroscedastic {{Bayesian}} Optimisation},
  author = {Griffiths, Ryan-Rhys and A Aldrick, Alexander and {Garcia-Ortegon}, Miguel and Lalchand, Vidhi and Lee, Alpha A},
  year = {2022},
