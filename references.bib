  title = {Adaptive Artificial Evolution of Droplet Protocells in a {{3D-printed}} Fluidic Chemorobotic Platform with Configurable Environments},
  author = {{Parrilla-Gutierrez}, Juan Manuel and Tsuda, Soichiro and Grizou, Jonathan and Taylor, James and Henson, Alon and Cronin, Leroy},
  year = {2017},
