@article{bedard_reconfigurable_2018,
  title = {Reconfigurable System for Automated Optimization of Diverse Chemical Reactions},
  author = {B{\'e}dard, Anne-Catherine and Adamo, Andrea and Aroh, Kosi C. and Russell, M. Grace and Bedermann, Aaron A. and Torosian, Jeremy and Yue, Brian and Jensen, Klavs F. and Jamison, Timothy F.},
