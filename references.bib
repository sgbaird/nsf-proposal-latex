@article{gromski_how_2019,
  type = {Review},
  title = {How to Explore Chemical Space Using Algorithms and Automation},
  author = {Gromski, Piotr S. and Henson, Alon B. and Granda, Jaroslaw M. and Cronin, Leroy},
  year = {2019},
  month = feb,
  journal = {NATURE REVIEWS CHEMISTRY},
  volume = {3},
