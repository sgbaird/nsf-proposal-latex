@article{ghoreishi_multi-information_2018,
  title = {Multi-{{Information Source Fusion}} and {{Optimization}} to {{Realize ICME}}: {{Application}} to {{Dual-Phase Materials}}},
  shorttitle = {Multi-{{Information Source Fusion}} and {{Optimization}} to {{Realize ICME}}},
  author = {Ghoreishi, Seyede Fatemeh and Molkeri, Abhilash and Srivastava, Ankit and Arroyave, Raymundo and Allaire, Douglas},
