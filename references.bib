  author = {Deneault, James R. and Chang, Jorge and Myung, Jay and Hooper, Daylond and Armstrong, Andrew and Pitt, Mark and Maruyama, Benji},
  year = {2021},
  month = jul,
  journal = {MRS Bulletin},
  volume = {46},
